<%= button_to(
        "Log Out",
        destroy_user_session_path,
        method: :delete
      ) %>

<%= form_with model: Tweet.new, url: tweets_path, local: true do |f| %>
  <%= f.text_field :body %>
  <%= f.submit %>
<% end %>

<ul>
  <% Tweet.find_each do |tweet| %>
    <%= tweet.body %>
  <% end %>
</ul>