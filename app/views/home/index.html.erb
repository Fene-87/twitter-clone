<% if user_signed_in? %>
<%= button_to(
        "Log Out",
        destroy_user_session_path,
        method: :delete
      ) %>
<% else %>
  <h1>Hello World</h1>
  <%= link_to "Signup", new_user_registration_path, class: "primary-button" %>
  <%= link_to "Login", new_user_session_path, class: "login-button" %>
<% end %>